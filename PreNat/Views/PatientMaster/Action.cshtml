@model PreNat.ViewModels.MainActionViewModel

<style>
    .form-control {
        border: solid 1px black;
        height: 40px;
    }

    .nav-pills.nav-pills-custom .nav-link.active {
        background: #5E6C90;
        margin-right: -15px;
        color: white;
    }

    .nav-pills.nav-pills-custom .nav-link {
        border-radius: 0px;
        margin-right: -15px;
        padding: 0.5rem 2.5rem;
        background: #45BC9B;
        color: white;
    }

    .form-check text-center {
        position: relative;
        display: block;
        width: 160px;
        margin-top: 0;
        margin-bottom: 0;
        padding-left: 0;
    }

    .form-check {
        position: relative;
        display: block;
        width: 160px;
        margin-top: 0;
        margin-bottom: 0;
        padding-left: 0;
    }

    .form-select {
        display: block;
        width: 100%;
        padding: 0.375rem 2.25rem 0.375rem 0.75rem;
        --moz-padding-start: calc(0.75rem - 3px);
        font-size: 1rem;
        font-weight: 400;
        line-height: 1.5;
        color: #212529;
        background-color: #fff;
        background-repeat: no-repeat;
        background-position: right 0.75rem center;
        background-size: 16px 12px;
        border: 1px solid black;
        border-radius: 0.25rem;
        transition: border-color 0.15s ease-in-out, box-shadow 0.15s ease-in-out;
        appearance: none;
    }
</style>
@if (Model != null)
{

    <div class="card">
        <div class="card-body">
            <h2 class="card-title">
                @if (Model.ID != 0)
                {
                    <span>Edit Patient Master</span>
                }
                else
                {
                    <span>Save Patient Master</span>
                }
            </h2>
            <hr />


            <div class="col-12">
                <div class="card">

                    <div class="card-body">
                        <div class="row">
                            <div class="col-md-12 mx-auto">
                                @if (Model.ID != 0)
                                {
                                    <ul class="nav nav-pills nav-pills-custom" id="pills-tab-custom" role="tablist">
                                        <li class="nav-item">
                                            <a class="nav-link active" id="pills-pi-tab-custom" data-bs-toggle="pill" href="#pills-pi" role="tab" aria-bs-controls="pills-pi" aria-bs-selected="true">
                                                Step 1: Patient Identification
                                            </a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" id="pills-cop-tab-custom" data-bs-toggle="pill" href="#pills-cop" role="tab" aria-bs-controls="pills-cop" aria-bs-selected="false">
                                                Step 2: Confirmation of Preganancy
                                            </a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" id="pills-b-tab-custom" data-bs-toggle="pill" href="#pills-b" role="tab" aria-bs-controls="pills-b" aria-bs-selected="false">
                                                Step 3: Background
                                            </a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" id="pills-cg-tab-custom" data-bs-toggle="pill" href="#pills-cg" role="tab" aria-bs-controls="pills-cg" aria-bs-selected="false">
                                                Step 4: Current Gestation
                                            </a>
                                        </li>

                                        <li class="nav-item">
                                            <a class="nav-link" id="pills-ogcc-tab-custom" data-bs-toggle="pill" href="#pills-ogcc" role="tab" aria-bs-controls="pills-ogcc" aria-bs-selected="false">
                                                Step 5: Obstetric Gynecology Clinical Condition
                                            </a>
                                        </li>

                                        <li class="nav-item">
                                            <a class="nav-link" id="pills-cp-tab-custom" data-bs-toggle="pill" href="#pills-cp" role="tab" aria-bs-controls="pills-cp" aria-bs-selected="false">
                                                Step 6: Chronlogical Pregnancy
                                            </a>
                                        </li>

                                        <li class="nav-item">
                                            <a class="nav-link" id="pills-pa-tab-custom" data-bs-toggle="pill" href="#pills-pa" role="tab" aria-bs-controls="pills-pa" aria-bs-selected="false">
                                                Step 7: Psychosocial Assessment
                                            </a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" id="pills-imm-tab-custom" data-bs-toggle="pill" href="#pills-imm" role="tab" aria-bs-controls="pills-imm" aria-bs-selected="false">
                                                Step 8: Immunizations
                                            </a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" id="pills-lb-tab-custom" data-bs-toggle="pill" href="#pills-lb" role="tab" aria-bs-controls="pills-lb" aria-bs-selected="false">
                                                Step 9: Laborotory Tests
                                            </a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" id="pills-r-tab-custom" data-bs-toggle="pill" href="#pills-r" role="tab" aria-bs-controls="pills-r" aria-bs-selected="false">
                                                Step 10: Report
                                            </a>
                                        </li>
                                    </ul>
                                }
                                else
                                {
                                    <input type="hidden" id="PatientID" value="" />
                                    <ul class="nav nav-pills nav-pills-custom" id="pills-tab-custom" role="tablist">
                                        <li class="nav-item">
                                            <a class="nav-link active" id="pills-pi-tab-custom" data--toggle="pill" href="#pills-pi" role="tab" aria-controls="pills-pi" aria-selected="true">
                                                Step 1: Patient Identification
                                            </a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" id="pills-cop-tab-custom" data-toggle="pill" href="#pills-cop" role="tab" aria-controls="pills-cop" aria-selected="false">
                                                Step 2: Confirmation of Preganancy
                                            </a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" id="pills-b-tab-custom" data-toggle="pill" href="#pills-b" role="tab" aria-controls="pills-b" aria-selected="false">
                                                Step 3: Background
                                            </a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" id="pills-cg-tab-custom" data-toggle="pill" href="#pills-cg" role="tab" aria-controls="pills-cg" aria-selected="false">
                                                Step 4:  Current Gestation
                                            </a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" id="pills-ogcc-tab-custom" data-toggle="pill" href="#pills-ogcc" role="tab" aria-controls="pills-ogcc" aria-selected="false">
                                                Step 5:  Obstetric Gynecology Clinical Condition
                                            </a>
                                        </li>

                                        <li class="nav-item">
                                            <a class="nav-link" id="pills-cp-tab-custom" data-toggle="pill" href="#pills-cp" role="tab" aria-controls="pills-cp" aria-selected="false">
                                                Step 6: Chronlogical Pregnancy
                                            </a>
                                        </li>

                                        <li class="nav-item">
                                            <a class="nav-link" id="pills-pa-tab-custom" data-toggle="pill" href="#pills-pa" role="tab" aria-controls="pills-pa" aria-selected="false">
                                                Step 7: Psychosocial Assessment
                                            </a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" id="pills-imm-tab-custom" data-toggle="pill" href="#pills-imm" role="tab" aria-controls="pills-imm" aria-selected="false">
                                                Step 8: Immunizations
                                            </a>
                                        </li>

                                        <li class="nav-item">
                                            <a class="nav-link" id="pills-lb-tab-custom" data-toggle="pill" href="#pills-lb" role="tab" aria-controls="pills-lb" aria-selected="false">
                                                Step 9: Laborotory Tests
                                            </a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" id="pills-r-tab-custom" data-toggle="pill" href="#pills-r" role="tab" aria-controls="pills-r" aria-selected="false">
                                                Step 10: Report
                                            </a>
                                        </li>
                                    </ul>
                                }


                                <br />
                                <div class="tab-content tab-content-custom-pill" id="pills-tabContent-custom">
                                    <div class="tab-pane fade show active" id="pills-pi" role="tabpanel" aria-labelledby="pills-pi-tab-custom">
                                        <h3 class="text-center">
                                            Step 1: Patient Identification
                                        </h3>
                                        <hr />
                                        <form id="patientIdForm">
                                            <div class="row">
                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        <label for="name">Name</label>
                                                        <input type="text" required class="form-control" value="@Model.Name" name="name" id="name">
                                                    </div>

                                                </div>
                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        <label for="surnames">Surnames</label>
                                                        <input type="text" required class="form-control" value="@Model.Surnames" name="surnames" id="surnames">

                                                    </div>
                                                </div>

                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label for="type_of_identity">Type of Identity</label>
                                                        <select required class="form-select" name="type_of_identity" id="type_of_identity">
                                                            <option value="Cedula de Ciudadanía" @(Model.Type_of_Identity == "Cedula de Ciudadanía" ? "selected" : "")>Cedula de Ciudadanía</option>
                                                            <option value="Cédula de Extranjería" @(Model.Type_of_Identity == "CE" ? "selected" : "")>Cédula de Extranjería</option>
                                                            <option value="Tarjeta de Identidad" @(Model.Type_of_Identity == "TI" ? "selected" : "")>Tarjeta de Identidad</option>
                                                        </select>
                                                    </div>

                                                </div>
                                            </div>

                                            <div class="row">
                                                <div class="col-md-3">

                                                    <div class="form-group">
                                                        <label for="department_of_residence">Department of Residence</label>
                                                        <input type="text" required class="form-control" value="@Model.Department_of_residence" name="department_of_residence" id="department_of_residence">
                                                    </div>
                                                </div>

                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        <label for="address_of_residence">Address of Residence</label>
                                                        <input type="text" required class="form-control" value="@Model.Address_of_residence" name="address_of_residence" id="address_of_residence">
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label for="city_municipality_of_residence">City/Municipality of Residence</label>
                                                        <input type="text" required class="form-control" value="@Model.City_Municipality_of_residence" name="city_municipality_of_residence" id="city_municipality_of_residence">
                                                    </div>

                                                </div>

                                            </div>
                                            <div class="row">
                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        <label for="telephones_landline">Telephones (Landline)</label>
                                                        <input type="text" required class="form-control" value="@Model.Telephones_Landline" name="telephones_landline" id="telephones_landline">
                                                    </div>

                                                </div>
                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        <label for="cell_phone">Cell Phone</label>
                                                        <input type="text" required class="form-control" value="@Model.Cell_Phone" name="cell_phone" id="cell_phone">
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label for="Date_of_birth">Date of Birth</label>
                                                        <input style="background-color:#FFFF00;" type="date" class="form-control" value="@Model.Date_of_birth.ToString("yyyy-MM-dd")" name="Date_of_birth" id="Date_of_birth">
                                                    </div>

                                                </div>
                                                <div class="col-md-2">
                                                    <div class="form-group">
                                                        <label for="age">Age</label>
                                                        <input required type="number" class="form-control" value="@Model.Age" name="age" id="age">
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label for="race_dane_information">Race (DANE Information)</label>
                                                        <select required class="form-select" name="race_dane_information" id="race_dane_information">
                                                            <option value="Black" @(Model.Race_DANE_Information == "Black" ? "selected" : "")>Black (Afro-descendant)</option>
                                                            <option value="Mestizo" @(Model.Race_DANE_Information == "Mestizo" ? "selected" : "")>Mestizo</option>
                                                            <option value="White" @(Model.Race_DANE_Information == "White" ? "selected" : "")>White</option>
                                                            <option class="Yellow" style="background:#FFFF00;" value="Indigenous" @(Model.Race_DANE_Information == "Indigenous" ? "selected" : "")>Indigenous</option>
                                                        </select>
                                                    </div>

                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label for="ethnicity_dane_information">Ethnicity (DANE Information)</label>
                                                        <select required class="form-select" name="ethnicity_dane_information" id="ethnicity_dane_information">
                                                            <option value="Palenquera" @(Model.Ethnicity_DANE_Information == "Palenquera" ? "selected" : "")>Palenquera</option>
                                                            <option value="Raizal" @(Model.Ethnicity_DANE_Information == "Raizal" ? "selected" : "")>Raizal</option>
                                                            <option class="Yellow" style="background:#FFFF00;" value="Indigenous" @(Model.Ethnicity_DANE_Information == "Indigenous" ? "selected" : "")>Indigenous</option>
                                                            <option class="Yellow" style="background:#FFFF00;" value="Gypsy" @(Model.Ethnicity_DANE_Information == "Gypsy" ? "selected" : "")>Gypsy</option>
                                                        </select>
                                                    </div>
                                                </div>

                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label for="level_of_education">Level of Education</label>
                                                        <select required class="form-select" name="level_of_education" id="level_of_education">
                                                            <option value="Primary" @(Model.Level_of_Education == "Primary" ? "selected" : "")>Primary</option>
                                                            <option value="Bachelor" @(Model.Level_of_Education == "Bachelor" ? "selected" : "")>Bachelor</option>
                                                            <option value="Technical" @(Model.Level_of_Education == "Technical" ? "selected" : "")>Technical</option>
                                                            <option value="Professional" @(Model.Level_of_Education == "Professional" ? "selected" : "")>Professional</option>
                                                        </select>
                                                    </div>
                                                </div>

                                            </div>

                                            <div class="row">
                                                <div class="col-md-4">

                                                    <div class="form-group">
                                                        <label for="marital_status">Marital Status</label>
                                                        <select required class="form-select" name="marital_status" id="marital_status">
                                                            <option value="Married" @(Model.Marital_status == "Married" ? "selected" : "")>Married</option>
                                                            <option value="Single" @(Model.Marital_status == "Single" ? "selected" : "")>Single</option>
                                                            <option value="Separated" @(Model.Marital_status == "Separated" ? "selected" : "")>Separated</option>
                                                            <option value="Free Union" @(Model.Marital_status == "Free Union" ? "selected" : "")>Free Union</option>
                                                            <option value="Divorced" @(Model.Marital_status == "Divorced" ? "selected" : "")>Divorced</option>
                                                            <option value="Widowed" @(Model.Marital_status == "Widowed" ? "selected" : "")>Widowed</option>
                                                        </select>
                                                    </div>
                                                </div>

                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label for="affiliation_regime">Affiliation Regime</label>
                                                        <select required class="form-select" name="affiliation_regime" id="affiliation_regime">
                                                            <option value="Subsidized" @(Model.Affiliation_regime == "Subsidized" ? "selected" : "")>Subsidized</option>
                                                            <option value="Contributory" @(Model.Affiliation_regime == "Contributory" ? "selected" : "")>Contributory</option>
                                                        </select>

                                                    </div>

                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label for="eps_ips">EPS/IPS</label>
                                                        <input type="text" required class="form-control" value="@Model.EPS_IPS" name="eps_ips" id="eps_ips">
                                                    </div>
                                                </div>
                                            </div>



                                        </form>
                                        <hr />
                                        <div class="row">
                                            <button type="button" id="PatientIDActionBtn" class="btn btn-block btn-primary">
                                                Save and Continue
                                            </button>
                                        </div>
                                    </div>


                                    <div class="tab-pane fade" id="pills-cop" role="tabpanel" aria-labelledby="pills-cop-tab-custom">
                                        <h3 class="text-center">
                                            Step 2: Pregnancy Confirmation
                                        </h3>
                                        <hr />
                                        <form id="pocForm">
                                            <div class="row">
                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        <label for="fum">FUM</label>
                                                        <input type="date" class="form-control" value="@Model.FUM.ToString("yyyy-MM-dd")" name="FUM" id="FUM">
                                                    </div>

                                                </div>
                                                <div class="col-md-2">
                                                    <div class="form-group">
                                                        <label for="age">Weeks Pregnant</label>
                                                        <input required type="text" class="form-control" value="@Model.WeeksPregnant" name="WeeksPregnant" id="WeeksPregnant">
                                                    </div>
                                                </div>
                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        <label for="date_of_birth">Positive HCG Presence</label>
                                                        <input type="date" class="form-control" value="@Model.Positive_HCG_presence_Date.ToString("yyyy-MM-dd")" name="Positive_HCG_presence_Date" id="Positive_HCG_presence_Date">
                                                    </div>

                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label>Attachment</label>
                                                        <input class="form-control" value="@Model.Upload_document1" name="Upload_document1" id="Upload_document1" type="hidden" />
                                                        <input class="form-control" id="FileAttachmentUpload" name="FileAttachmentUpload" type="file" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">


                                            </div>

                                            <div class="row">
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label for="date_of_birth">Positive TV Ultrasound Date</label>
                                                        <input type="date" class="form-control" value="@Model.Positive_TV_ultrasound_Date.ToString("yyyy-MM-dd")" name="Positive_TV_ultrasound_Date" id="Positive_TV_ultrasound_Date">
                                                    </div>

                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label>Attachment</label>
                                                        <input class="form-control" value="@Model.Upload_document2" name="Upload_document2" id="Upload_document2" type="hidden" />
                                                        <input class="form-control" id="FileAttachmentUpload2" name="FileAttachmentUpload2" type="file" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label for="affiliation_regime">Pregnancy Confirm</label>
                                                        <select required class="form-select" name="PregnancyConfirm" id="PregnancyConfirm">
                                                            <option value="Yes" @(Model.PregnancyConfirm == "Yes" ? "selected" : "")>Yes</option>
                                                            <option value="No" @(Model.PregnancyConfirm == "No" ? "selected" : "")>No</option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>







                                        </form>
                                        <hr />
                                        <div class="row">
                                            <button type="button" id="PregConfirmActionBtn" class="btn btn-block btn-primary">
                                                Save and Continue
                                            </button>
                                        </div>
                                    </div>


                                    <div class="tab-pane fade" id="pills-b" role="tabpanel" aria-labelledby="pills-b-tab-custom">
                                        <h3 class="text-center">
                                            Step 3: Background
                                        </h3>
                                        <hr />
                                        <form id="backgroundForm">
                                            <div class="table-responsive">
                                                <table class="table table-bordered">
                                                    <thead>
                                                        <tr>
                                                            <td class="text-center">
                                                                General Pathologies
                                                            </td>
                                                            <td class="text-center">
                                                                Absence of Pathologies
                                                            </td>
                                                            <td class="text-center">
                                                                Family
                                                            </td>
                                                            <td class="text-center">
                                                                Personal
                                                            </td>

                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr>
                                                            <td>
                                                                Tbc
                                                            </td>
                                                            <td style="background-color: #93C47D;"><input class="form-check text-center" type="checkbox" id="tbc-absent-checkbox"></td>
                                                            <td style="background-color:#FFFF00;"><input class="form-check text-center" type="checkbox" id="tbc-present-family-checkbox"></td>
                                                            <td style="background-color: #FF0000"><input class="form-check text-center" type="checkbox" id="tbc-present-personally-checkbox"></td>
                                                            <td hidden><input type="hidden" id="Tbc" name="Tbc" value="@Model.Tbc" /></td>
                                                        <tr>
                                                            <td>
                                                                Diabetes
                                                            </td>
                                                            <td style="background-color: #93C47D;"><input class="form-check text-center" type="checkbox" id="diabetes-absent-checkbox"></td>
                                                            <td style="background-color:#FFFF00;"><input class="form-check text-center" type="checkbox" id="diabetes-present-family-checkbox"></td>
                                                            <td style="background-color: #FF0000"><input class="form-check text-center" type="checkbox" id="diabetes-present-personally-checkbox"></td>
                                                            <td hidden><input type="hidden" id="Diabetes" name="Diabetes" value="@Model.Diabetes" /></td>

                                                        </tr>

                                                        <tr>
                                                            <td>
                                                                Hypertension
                                                            </td>
                                                            <td style="background-color: #93C47D;"><input class="form-check text-center" type="checkbox" id="hypertension-absent-checkbox"></td>
                                                            <td style="background-color:#FFFF00;"><input class="form-check text-center" type="checkbox" id="hypertension-present-family-checkbox"></td>
                                                            <td style="background-color: #FF0000"><input class="form-check text-center" type="checkbox" id="hypertension-present-personally-checkbox"></td>
                                                            <td hidden><input type="hidden" id="Hypertension" name="Hypertension" value="@Model.Hypertension" /></td>


                                                        </tr>

                                                        <tr>
                                                            <td>
                                                                Eclampsia
                                                            </td>
                                                            <td style="background-color: #93C47D;"><input class="form-check text-center" type="checkbox" id="eclampsia-absent-checkbox"></td>
                                                            <td style="background-color:#FFFF00;"><input class="form-check text-center" type="checkbox" id="eclampsia-present-family-checkbox"></td>
                                                            <td style="background-color: #FF0000"><input class="form-check text-center" type="checkbox" id="eclampsia-present-personally-checkbox"></td>
                                                            <td hidden><input type="hidden" id="Eclampsia" name="Eclampsia" value="@Model.Eclampsia" /></td>

                                                        </tr>

                                                        <tr>
                                                            <td>
                                                                Preeclampsia
                                                            </td>
                                                            <td style="background-color: #93C47D;"><input class="form-check text-center" type="checkbox" id="preeclampsia-absent-checkbox"></td>
                                                            <td style="background-color:#FFFF00;"><input class="form-check text-center" type="checkbox" id="preeclampsia-present-family-checkbox"></td>
                                                            <td style="background-color: #FF0000"><input class="form-check text-center" type="checkbox" id="preeclampsia-present-personally-checkbox"></td>
                                                            <td hidden><input type="hidden" id="Preeclampsia" name="Preeclampsia" value="@Model.Preeclampsia" /></td>

                                                        </tr>

                                                        <tr>
                                                            <td>
